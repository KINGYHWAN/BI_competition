 = round(result['기온'],4)\n","  result['강수량'] = round(result['강수량'],4)\n","  result['습도'] = round(result['습도'],4)\n","  result['일조'] = round(result['일조'],4)\n","  result['일사'] = round(result['일사'],4)\n","  result['적설'] = round(result['적설'],4)\n","\n","  result.drop(['지역명'],axis=1, inplace=True)\n","\n","  return result"],"metadata":{"id":"m-bLhNNtaoJ-"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# 데이터프레임 라벨링\n","final_df=[weighted_Gw_df, weighted_Gg_df, weighted_GN_df, weighted_GB_df, weighted_gwangju_df, weighted_daegu_df, weighted_daejeon_df, weighted_busan_df, weighted_seoul_df, weighted_woolsan_df,weighted_incheon_df, weighted_JN_df, weighted_JB_df, weighted_Jj_df, weighted_CN_df, weighted_CB_df]\n","\n","# 더하기 함수 적용\n","weather_df=second_df_calculate(final_df)\n","\n","#확인\n","weather_df"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":423},"id":"0tTY3t1djIPs","executionInfo":{"status":"ok","timestamp":1687567336375,"user_tz":-540,"elapsed":11,"user":{"displayName":"정민균","userId":"02008085652330605669"}},"outputId":"2f3abe12-16ce-42a5-cc43-04357c95688b"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["                         시간       기온  강수량       습도   일조   일사      적설\n","0       2012-01-01 00:00:00  -1.0636  0.0  65.8577  0.0  0.0  0.0011\n","1       2012-01-01 00:05:00  -1.0852  0.0  65.9945  0.0  0.0  0.0011\n","2       2012-01-01 00:10:00  -1.1055  0.0  66.1311  0.0  0.0  0.0011\n","3       2012-01-01 00:15:00  -1.1270  0.0  66.2654  0.0  0.0  0.0011\n","4       2012-01-01 00:20:00  -1.1482  0.0  66.4029  0.0  0.0  0.0011\n","...                     ...      ...  ...      ...  ...  ...     ...\n","1182812 2023-03-30 23:40:00  10.2100  0.0  50.1872  0.0  0.0  0.0000\n","1182813 2023-03-30 23:45:00  10.1441  0.0  50.3786  0.0  0.0  0.0000\n","1182814 2023-03-30 23:50:00  10.0753  0.0  50.5654  0.0  0.0  0.0000\n","1182815 2023-03-30 23:55:00  10.0092  0.0  50.7568  0.0  0.0 